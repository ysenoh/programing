#!/usr/bin/python3
#  --*-coding:utf-8-*--


def main():
    T = int(input())

    for i in range(T):
        R,C,H,V = map(int, input().split())
        lines = [input() for i in range(R)]
        
        print('Case #' + str(i+1) + ': ' + str(f(lines, R, C, H, V)))


if __name__ == '__main__':
    main()
